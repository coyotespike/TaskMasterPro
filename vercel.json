{
  "version": 2,
  "buildCommand": "bash .vercel/build.sh",
  "outputDirectory": "dist",
  "functions": {
    "api/index.ts": {
      "includeFiles": "dist/public/**"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api" },
    { "source": "/(.*)", "destination": "/api" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}